<h2>Available drones</h2>
<a href="/drones/create">Add Drone</a>
{{#each drones}}
    <div>
        <h3>Name: {{name}}</h3>
        <ul>
            <li>Propells: {{propells}}</li>
            <li>Max Speed: {{maxSpeed}}</li>
        </ul>
        <a href="/drones/{{id}}/edit">Edit</a>
        <form action="/drones/{{id}}/delete" method="POST">
            <button>Delete</button>
        </form>
    </div>
{{/each}}